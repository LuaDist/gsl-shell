class Point
   self(x = 0, y = 0)
      print(`
         x: ${x},
         y: ${y}
      `)
      self.move(x, y)
   end
   move(x, y)
      self.x = x
      self.y = y
   end
end

class Point3D extends Point
   self(x, y, z = 0)
      super(x, y)
      self.z = z
   end
   move(x, y, z)
      super.move(x, y)
      self.z = z
   end
end

Object.defineProperties(Point, {
   greet = {
      value = function(self, whom)
         print(`Hello ${whom}`)
      end
   }
})

p = Point()
p.greet("Rich")
p = Point3D(1,2,3)
print(p.x, p.y, p.z)

print("is check 1:", p is Point)
print("is check 2:", p is Point3D)
